//2. Write a program to copy one string to another string without using library function

#include<bits/stdc++.h>
using namespace std;

int main(){
    char original[100];
    cout<<"Enter original input : ";
    cin>>original;
    int n = 0;
    for(int i=0;original[i]!='\0';i++){
        n++;
    }
    char copied[100];
for(int i=0;i<n;i++){
    copied[i] = original[i];
}
cout<<"Copied string is : "<<copied;
    
    return 0;
}
